{
  "name": "rfh",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "develop": "nuxt",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "serve": "nuxt",
    "build": "nuxt build",
    "build:docs": "vue-styleguidist build",
    "serve:prod": "nuxt start",
    "serve:docs": "vue-styleguidist server",
    "generate": "env NODE_ENV=mobile nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "build:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && ./gradlew bundleRelease && ./gradlew app:assembleRelease",
    "run:ios": "npm run generate && npx cap copy && npx cap open ios",
    "run:android": "npm run generate && npx cap copy && npx cap open android",
    "create-component": "node index.cli.js component",
    "create-page": "node index.cli.js page",
    "create-store": "node index.cli.js store"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@capacitor/android": "^2.4.7",
    "@capacitor/cli": "^3.0.0-rc.0",
    "@capacitor/core": "^3.0.0-rc.0",
    "@capacitor/ios": "^2.4.7",
    "@capacitor/share": "^0.5.3",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/strapi": "^0.3.1",
    "core-js": "^3.9.1",
    "highlight.js": "^10.7.2",
    "markdown-it-highlightjs": "^3.4.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.3",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.0.5",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^4.3.8",
    "inquirer": "^8.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "nuxt-build-optimisations": "^1.0.2",
    "prettier": "^2.2.1",
    "sass-loader": "10.1.1",
    "sass-resources-loader": "^2.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.4",
    "vue-jest": "^3.0.4",
    "vue-styleguidist": "^4.38.0"
  }
}
